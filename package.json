{
  "name": "app",
  "version": "2.0.0",
  "dependencies": {
    "@webserverless/fc-express": "^0.1.1",
    "express": "^4.17.1",
    "react": "16.8.3",
    "react-dom": "16.8.3",
    "react-router-dom": "^5.0.0",
    "ykfe-utils": "^2.0.2"
  },
  "scripts": {
    "start": "concurrently \"NODE_ENV=development fun local start\" \"cross-env NODE_ENV=development webpack --watch --config ./build/webpack.config.server.js\"",
    "prod": "npm run build && eggctl start",
    "deploy": "npm run build && rimraf node_modules && npm i --production && fun deploy",
    "build:server": "cross-env NODE_ENV=production webpack --config build/webpack.config.server.js",
    "build:client": "cross-env NODE_ENV=production ykcli build",
    "build": "rimraf dist && npm run build:client && npm run build:server",
    "analyze": "cross-env NODE_ENV=production npm_config_report=true ykcli build"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 9",
    "not op_mini all"
  ],
  "devDependencies": {
    "yk-cli": "^2.0.0",
    "@babel/core": "^7.4.4",
    "@babel/plugin-transform-runtime": "^7.5.5",
    "@babel/polyfill": "^7.4.4",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.0.0",
    "@babel/runtime": "^7.5.5",
    "babel-loader": "8.0.4",
    "browserslist": "^4.6.3",
    "concurrently": "^4.1.0",
    "cross-env": "^5.2.0",
    "css-hot-loader": "^1.4.3",
    "css-loader": "1.0.0",
    "css-modules-require-hook": "^4.2.3",
    "file-loader": "2.0.0",
    "less": "^3.9.0",
    "less-loader": "^4.1.0",
    "memory-fs": "^0.4.1",
    "mini-css-extract-plugin": "^0.5.0",
    "nodemon": "^1.19.1",
    "express-http-proxy": "^1.6.0",
    "optimize-css-assets-webpack-plugin": "5.0.1",
    "postcss-flexbugs-fixes": "4.1.0",
    "postcss-loader": "3.0.0",
    "postcss-preset-env": "^6.0.5",
    "postcss-safe-parser": "4.0.1",
    "rimraf": "^2.6.3",
    "react-dev-utils": "^8.0.0",
    "terser-webpack-plugin": "^1.2.0",
    "url-loader": "1.1.1",
    "webpack": "4.19.1",
    "webpack-bundle-analyzer": "^3.0.3",
    "webpack-cli": "^3.3.3",
    "webpack-dev-server": "3.3.1",
    "webpack-manifest-plugin": "^2.0.4",
    "webpack-merge": "^4.1.4",
    "webpack-node-externals": "^1.7.2"
  }
}
