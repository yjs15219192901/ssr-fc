{
  "name": "app",
  "version": "2.0.0",
  "dependencies": {
    "@webserverless/fc-express": "0.1.3",
    "express": "4.17.1",
    "react": "16.12.0",
    "react-dom": "16.12.0",
    "react-router-dom": "^5.0.0",
    "stream-to-string": "^1.2.0",
    "ykfe-utils": "^3.2.0"
  },
  "scripts": {
    "start": "concurrently \"npm run ssr\" \"npm run csr\"",
    "ssr": "concurrently \"fun local start\" \"cross-env NODE_ENV=development webpack --watch --config ./build/webpack.config.server.js\"",
    "csr": "cross-env NODE_ENV=development RUNTIME=serverless ykcli dev --PORT 8888",
    "debug": "concurrently \"fun local start -d 3000 --config vscode\" \"cross-env NODE_ENV=development webpack --watch --config ./build/webpack.config.server.js\"",
    "deploy": "npm run build && fun deploy",
    "build:server": "cross-env NODE_ENV=production webpack --display-used-exports --optimize-minimize --config build/webpack.config.server.js",
    "build:client": "cross-env NODE_ENV=production RUNTIME=serverless ykcli build",
    "build": "rimraf dist && npm run build:server && npm run build:client",
    "analyze": "cross-env NODE_ENV=production npm_config_report=true ykcli build"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 9",
    "not op_mini all"
  ],
  "devDependencies": {
    "@babel/core": "^7.4.4",
    "@babel/polyfill": "^7.4.4",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-react": "^7.0.0",
    "babel-loader": "^8.0.4",
    "browserslist": "^4.6.3",
    "concurrently": "^5.1.0",
    "cross-env": "^7.0.0",
    "css-hot-loader": "^1.4.3",
    "css-loader": "3.4.2",
    "css-modules-require-hook": "^4.2.3",
    "express-http-proxy": "^1.6.0",
    "file-loader": "5.0.2",
    "ignore-loader": "^0.1.2",
    "less": "^3.9.0",
    "less-loader": "^5.0.0",
    "mini-css-extract-plugin": "^0.9.0",
    "optimize-css-assets-webpack-plugin": "5.0.3",
    "postcss-flexbugs-fixes": "4.2.0",
    "postcss-loader": "3.0.0",
    "postcss-preset-env": "^6.0.5",
    "postcss-safe-parser": "4.0.2",
    "react-dev-utils": "^10.2.0",
    "rimraf": "^3.0.2",
    "terser-webpack-plugin": "^2.3.5",
    "url-loader": "3.0.0",
    "webpack": "^4.19.1",
    "webpack-bundle-analyzer": "^3.0.3",
    "webpack-cli": "^3.3.3",
    "webpack-dev-server": "3.10.3",
    "webpack-manifest-plugin": "^2.0.4",
    "webpack-merge": "^4.1.4"
  }
}
